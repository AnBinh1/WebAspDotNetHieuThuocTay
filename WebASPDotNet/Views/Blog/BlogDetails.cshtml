@model WebASPDotNet.Models.TblBlog
<div class="col-sm-3">
	@Html.Partial("_SidebarPartial")
</div>
<div class="col-sm-9">
		<div class="blog-post-area">
		<div class="single-blog-post" style="margin-left:50px">
			<h2 style="color:#FF6600">@Model.Title</h2>
						
			<p style="margin-left:50px">@Html.Raw(Model.Description)</p> <br>
			<p>@Html.Raw(Model.Detail)</p>
		</div>
	</div>
</div>